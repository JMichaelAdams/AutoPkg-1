<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the Ubertec Monitoring Client and imports into Munki.</string>
    <key>Identifier</key>
    <string>com.github.Ubertec.munki.UbertecMonitoringClient</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>MonitoringClient</string>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps/MonitoringClient</string>
        <key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
            <key>description</key>
            <string>Monitor your Clients Client's</string>
            <key>display_name</key>
            <string>MonitoringClient</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>minimum_os_version</key>
            <string>10.6</string>
            <key>unattended_install</key>
            <true/>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>ParentRecipe</key>
    <string>com.github.Ubertec.download.UbertecMonitoringClient</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>Unarchiver</string>
            <key>Arguments</key>
            <dict>
                <key>archive_path</key>
                <string>%pathname%</string>
                <key>destination_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%</string>
                <key>purge_destination</key>
                <true/>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>DmgCreator</string>
            <key>Arguments</key>
            <dict>
                <key>dmg_root</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%</string>
                <key>dmg_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%.dmg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>PkgInfoCreator</string>
        </dict>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>pkgtype</key>
                <string>bundle</string>
                <key>version</key>
                <string>6.1.4</string>
                <key>template_path</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%.dmg/MonitoringClient.pkg/Contents</string>
                <key>infofile</key>
                <string>%MUNKI_REPO_SUBDIR%/apps/MonitoringClient</string>
                <key>pkgroot</key>
                <string>%RECIPE_CACHE_DIR%/%NAME%.dmg/MonitoringClient.pkg</string>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
        </dict>
    </array>
</dict>
</plist>
